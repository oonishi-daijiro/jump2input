{
  "name": "Jump2Input",
  "version": "1.4",
  "manifest_version": 3,

  "description": "Ctrl+Shift+Yで現在のページの入力フィールドにジャンプします",
  "icons": {
    "128": "icon/128.png"
  },

  "background": {
    "service_worker": "./background.js"
  },

  "content_scripts": [
    {
      "js": ["./content_scripts/jump2input.js"],
      "matches": ["https://*/*", "http://*/*"],
      "run_at": "document_start"
    }
  ],

  "commands": {
    "jump to input": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "現在のページの入力要素にジャンプします"
    }
  },
  "permissions": ["commands"],
  "action": {},
  "host_permissions": ["http://*/*", "https://*/*"],
  "author": "Marujiro"
}
