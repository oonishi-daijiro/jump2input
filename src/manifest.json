{
  "name": "Jump2Input",
  "version": "1.1",
  "manifest_version": 3,

  "description": "Ctrl+Shift+Yで現在のページの入力フィールドにジャンプします",
  "icons": {
    "128": "./icon/128.png"
  },

  "background": {
    "service_worker": "./background.js"
  },

  "commands": {
    "jump to input": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "現在のページの入力要素にジャンプします"
    }
  },
  "action": {},
  "host_permissions": ["http://*/*", "https://*/*"],
  "permissions": ["scripting"],
  "author": "Marujiro"
}
